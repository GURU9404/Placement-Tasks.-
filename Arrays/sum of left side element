import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       
        Scanner sc =new Scanner(System.in);
        int sum=0;
        int n =sc.nextInt();
        int arr[]=new int [n];
        for(int i=0;i<n;i++){
             System.out.print(sum+" ");
            arr[i]=sc.nextInt();
            sum=sum+arr[i];
        }
            
      
    }
}
